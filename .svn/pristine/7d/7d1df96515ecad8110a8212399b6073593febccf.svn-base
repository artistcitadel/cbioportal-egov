  `PER_NAME` varchar(12) DEFAULT NULL,
  `DEPT_CODE` varchar(6) DEFAULT NULL,
  `CODE` varchar(9) DEFAULT NULL,
  `ENAM` varchar(500) DEFAULT NULL,
  `KNAM` varchar(500) DEFAULT NULL,
  `SHARE_REQ_YN` varchar(1) DEFAULT NULL,
  `SHARE_REQ_DT` timestamp NULL DEFAULT NULL,
