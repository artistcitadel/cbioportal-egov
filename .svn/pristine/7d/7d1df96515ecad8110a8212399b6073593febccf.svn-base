  `UDT_DT` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '수정일자',
  `UDT_ID` varchar(20) DEFAULT NULL COMMENT '변경자 ID',
  `CRT_ID` varchar(20) DEFAULT NULL COMMENT '생성자 ID',
  `CRT_DT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '변경일자',
