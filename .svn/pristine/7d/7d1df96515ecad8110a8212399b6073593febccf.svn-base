  `END_TIME` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '종료시간',
  `EXTRACT_COUNT` varchar(10) DEFAULT NULL COMMENT '추출건수',
  `LOAD_COUNT` varchar(10) DEFAULT NULL COMMENT '적재건수',
  `STATE` varchar(50) DEFAULT NULL COMMENT '상태',
  `STATE_MSG` varchar(200) DEFAULT NULL COMMENT '상태메세지',
  PRIMARY KEY (`SEQ`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='웹접속로그';

CREATE TABLE `CC_ITEM_CATE` (
  `SEQ` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '항목분류(대) 일련번호',
  `CATE_NM` varchar(128) NOT NULL COMMENT '항목분류(대) 이름',
