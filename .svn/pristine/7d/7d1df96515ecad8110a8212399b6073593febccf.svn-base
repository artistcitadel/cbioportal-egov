  `CRT_DT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '변경일자',
  `UDT_DT` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '수정일자',
  PRIMARY KEY (`SEQ`)
) ENGINE=InnoDB AUTO_INCREMENT=30 DEFAULT CHARSET=utf8 COMMENT='연구항목분류(대)관리';

CREATE TABLE `CC_ITEM_CATE_DETL` (
  `SEQ` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '항목분류(중) 일련번호',
  `ITEM_CATE_SEQ` int(10) unsigned NOT NULL COMMENT '항목분류(대) 일련번호',
  `CATE_NM` varchar(128) NOT NULL COMMENT '항목분류(중) 이름',
  `ORDER` int(10) unsigned NOT NULL DEFAULT '1',
  `SCHEMA` varchar(128) NOT NULL COMMENT '스키마명',
  `TABLE` varchar(128) NOT NULL COMMENT '테이블명',
  `TABLE_COMMENT` varchar(512) DEFAULT NULL COMMENT '테이블설명',
  `UDT_DT` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '수정일자',
  `UDT_ID` varchar(20) DEFAULT NULL COMMENT '변경자 ID',
  `CRT_ID` varchar(20) DEFAULT NULL COMMENT '생성자 ID',
  `CRT_DT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '변경일자',
  `BASE_DT_COLUMN` varchar(128) DEFAULT NULL COMMENT '테이블 기준일 컬럼명',
  `INSTCD_YN` char(1) DEFAULT NULL COMMENT '사업장적용여부',
  PRIMARY KEY (`SEQ`),
  KEY `CC_ITEM_CATE_DETL_CC_ITEM_CATE_FK` (`ITEM_CATE_SEQ`)
) ENGINE=InnoDB AUTO_INCREMENT=1003 DEFAULT CHARSET=utf8 COMMENT='연구항목분류(중)관리';

CREATE TABLE `CC_ITEM_CONT` (
  `SEQ` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '일련번호',
  `SHARE_CD` char(1) NOT NULL DEFAULT 'A' COMMENT '공유조건(P:개인, A:전체, D:과)',
  `CONDT_NM` varchar(512) NOT NULL COMMENT '연구조건명',
  `PER_CODE` varchar(20) NOT NULL COMMENT '사용자ID',
  `METH_CD` char(2) NOT NULL DEFAULT 'CS' COMMENT '연구종류(CS:단면, CC:사레대조, CH:코호트)',
  `CC_POP_NUM` int(10) unsigned DEFAULT NULL COMMENT '(사례대조) 모집단 숫자',
  `CC_CASE_NUM` int(10) unsigned DEFAULT NULL COMMENT '(사례대조) 사례군 숫자',
  `CC_SAM_NUM` int(10) unsigned DEFAULT NULL COMMENT '(사례대조) 대조군 배수',
  `CC_CONT_CD` char(1) DEFAULT NULL COMMENT '(사례대조) 대조군 배수',
  `CC_MAT_CD` varchar(3) DEFAULT NULL COMMENT '(사례대조) 대조군 매칭, ''AM'' : Age Match, ''SM'' : Sex Match, ''ASM'' : Age Sex Match',
  `UDT_DT` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '수정일자',
  `CRT_DT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '변경일자',
  `SAVE_CD` char(2) DEFAULT NULL COMMENT 'SC : 환자조회, SI : 연구조회',
  `DEPT_CODE` varchar(20) DEFAULT NULL,
  `CC_AGE_NUM` int(10) unsigned DEFAULT NULL COMMENT '(사례대조) 대조군 매칭 Age 범위',
  `INSTCD` varchar(3) DEFAULT NULL COMMENT '사업장코드',
  PRIMARY KEY (`SEQ`),
  KEY `CC_ITEM_CONT_CC_PERINX_FK` (`PER_CODE`)
) ENGINE=InnoDB AUTO_INCREMENT=577 DEFAULT CHARSET=utf8 COMMENT='사용자연구조건저장';

CREATE TABLE `CC_ITEM_CONT_DATA` (
  `SEQ` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '데이터일련번호',
  `PER_CODE` varchar(200) NOT NULL COMMENT '사용자ID',
  `CONT_SEQ` int(10) unsigned NOT NULL COMMENT '사용자 연구조건 저장 일련번호',
  `DATA_NM` varchar(512) NOT NULL COMMENT '데이터명',
  `UDT_DT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '수정일자',
  `DATA_SEQ` int(10) unsigned NOT NULL DEFAULT '1' COMMENT '사용자 연구조건에 대한 데이터 일련번호. 프로그램에서 따야 함.',
  `CRT_DT` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '변경일자',
  `TABLE_ID` varchar(100) DEFAULT NULL,
  `TABLE_CNT` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '데이터의 갯수',
  `TABLE_SIZE` decimal(10,2) DEFAULT NULL COMMENT '사용자테이블크기',
  PRIMARY KEY (`SEQ`),
  KEY `CC_ITEM_CONT_DATA_CC_PERINX_FK` (`PER_CODE`),
  KEY `CC_ITEM_CONT_DATA_CC_ITEM_CONT_FK` (`CONT_SEQ`)
) ENGINE=InnoDB AUTO_INCREMENT=229 DEFAULT CHARSET=utf8 COMMENT='연구데이터저장관리';

CREATE TABLE `CC_ITEM_CONT_DATA_DETL` (
  `SEQ` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '데이터상세일련번호',
  `DATA_SEQ` int(10) unsigned NOT NULL COMMENT '데이터 저장 일련번호',
  `COLUMN_ID` varchar(512) NOT NULL COMMENT '컬럼ID',
  `COLUMN_COMMENT` varchar(512) NOT NULL COMMENT '컬럼명',
  `UDT_DT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '수정일자',
  `CRT_DT` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '변경일자',
  `OLAP_YN` char(1) DEFAULT 'Y',
  `ITEM_SEQ` int(10) unsigned DEFAULT NULL,
  `DATA_TYPE` varchar(128) DEFAULT NULL,
  `ITEM_TYPE` char(3) DEFAULT NULL,
  `ADD_ITEM_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '추가아이템 여부 Y:추가',
  `HIDDEN_YN` varchar(100) DEFAULT NULL COMMENT '코호트연구 Agg 전체 HIDDEN여부',
  PRIMARY KEY (`SEQ`)
