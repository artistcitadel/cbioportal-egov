  `COMM_CD_EXT2` varchar(50) DEFAULT NULL COMMENT '공통코드 EXT2',
  `COMM_CD_EXT3` varchar(50) DEFAULT NULL COMMENT '공통코드 EXT3',
  `COMM_CD_DESC` varchar(4000) DEFAULT NULL COMMENT '공통코드 설명',
  `SORT` int(10) unsigned NOT NULL COMMENT '공통코드 순서',
  `USE_YN` char(1) NOT NULL COMMENT '공통코드 사용여부',
  `CRT_DT` datetime NOT NULL COMMENT '생성일자',
  `CRT_ID` varchar(20) DEFAULT NULL COMMENT '생성자 ID',
  `UDT_DT` datetime NOT NULL COMMENT '수정일자',
  `UDT_ID` varchar(20) DEFAULT NULL COMMENT '변경자 ID',
  `COMM_GRP_ID` varchar(50) DEFAULT NULL,
  PRIMARY KEY (`SEQ`)
) ENGINE=InnoDB AUTO_INCREMENT=159 DEFAULT CHARSET=utf8 COMMENT='공통코드정보';

CREATE TABLE `CC_COMMON_CODE_GROUP` (
  `SEQ` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '공통코드그룹 일련번호',
  `COMM_GRP_NM` varchar(100) DEFAULT NULL COMMENT '공통코드그룹명',
  `CRT_DT` datetime NOT NULL COMMENT '생성일자',
  `CRT_ID` varchar(20) DEFAULT NULL COMMENT '생성자 ID',
  `UDT_DT` datetime NOT NULL COMMENT '수정일자',
  `UDT_ID` varchar(20) DEFAULT NULL COMMENT '변경자 ID',
  `COMM_GRP_ID` varchar(50) DEFAULT NULL,
  PRIMARY KEY (`SEQ`)
) ENGINE=InnoDB AUTO_INCREMENT=47 DEFAULT CHARSET=utf8 COMMENT='공통코드그룹정보';

CREATE TABLE `CC_DASHBOARD` (
  `SEQ` int(11) NOT NULL,
  `SERVICE_STAT_SQL` text COMMENT '서비스 상태 메시지 생성 SQL',
  `RESEARCH_TARGET_STAT_SQL` text COMMENT '연구대상 통계 정보 생성 SQL',
  `TITLE_1` varchar(128) DEFAULT NULL COMMENT 'Dashboard 대제목',
  `TITLE_2` varchar(128) DEFAULT NULL COMMENT 'Dashboard 소제목',
