CREATE TABLE `CC_AUTINX` (
  `AUT_CODE` varchar(20) NOT NULL COMMENT '권한코드',
  `AUT_NAME` varchar(50) NOT NULL COMMENT '권한명',
  `CRT_DT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '수정일자',
  `CRT_ID` varchar(20) NOT NULL COMMENT '생성자 ID',
  `UDT_DT` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '변경일자',
  `UDT_ID` varchar(20) NOT NULL COMMENT '변경자 ID',
  PRIMARY KEY (`AUT_CODE`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='권한정보';

CREATE TABLE `CC_BOARD` (
  `SEQ` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '일련번호',
  `BOARD_SEQ` int(10) unsigned NOT NULL COMMENT '게시판 SEQ',
  `TITLE` varchar(512) NOT NULL COMMENT '게시물 제목',
  `CONTENT` text COMMENT '본문',
  `VISIT` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '조회수',
  `CRT_DT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '입력일자',
  `UDT_DT` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '수정일자',
  `UDT_ID` varchar(20) DEFAULT NULL COMMENT '변경자 ID',
  `CRT_ID` varchar(20) DEFAULT NULL COMMENT '생성자 ID',
  PRIMARY KEY (`SEQ`),
  KEY `FK_CC_BOARD_CC_BOARD_MGMT` (`BOARD_SEQ`),
  CONSTRAINT `FK_CC_BOARD_CC_BOARD_MGMT` FOREIGN KEY (`BOARD_SEQ`) REFERENCES `CC_BOARD_MGMT` (`SEQ`) ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=163 DEFAULT CHARSET=utf8 COMMENT='게시판 데이터 관리';

CREATE TABLE `CC_BOARD_ATTACH` (
  `SEQ` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '일련번호',
  `DOCUMENT_SEQ` int(10) unsigned NOT NULL COMMENT '문서 일련번호',
  `BOARD_SEQ` int(10) unsigned NOT NULL COMMENT '게시판 SEQ',
  `ATTACH_PATH` varchar(512) NOT NULL COMMENT '첨부파일 경로',
  `ATTACH_SIZE` int(10) unsigned NOT NULL COMMENT '첨부파일 크기',
  `ORDER` int(10) unsigned NOT NULL DEFAULT '1' COMMENT '순서',
  `UDT_DT` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '수정일자',
  `UDT_ID` varchar(20) DEFAULT NULL COMMENT '변경자 ID',
  `CRT_ID` varchar(20) DEFAULT NULL COMMENT '생성자 ID',
  `CRT_DT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '변경일자',
  `SAVE_FILE_NAME` varchar(100) DEFAULT NULL COMMENT '실제 저장되는 파일명',
  `FILE_NAME` varchar(100) DEFAULT NULL COMMENT '원래 파일명',
  PRIMARY KEY (`SEQ`),
  KEY `FK_CC_BOARD_ATTACH_CC_BOARD` (`DOCUMENT_SEQ`),
  CONSTRAINT `FK_CC_BOARD_ATTACH_CC_BOARD` FOREIGN KEY (`DOCUMENT_SEQ`) REFERENCES `CC_BOARD` (`SEQ`) ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=47 DEFAULT CHARSET=utf8 COMMENT='게시판 첨부파일 관리';

CREATE TABLE `CC_BOARD_MGMT` (
  `SEQ` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '일련번호',
  `TITLE` varchar(128) NOT NULL COMMENT '게시판명',
  `LIST_COUNT_DASHBOARD` tinyint(3) unsigned NOT NULL DEFAULT '6' COMMENT 'Dashboard에 표출되는 목록 개수',
  `LIST_COUNT_COMMON` int(10) unsigned NOT NULL DEFAULT '10' COMMENT '목록페이지에 표출되는 목록 개수',
  `ATTACH_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '파일첨부 여부',
  `SHOW_DASHBOARD_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT 'Dashboard 표출여부',
  `ORDER_NUM` int(10) unsigned NOT NULL DEFAULT '1' COMMENT '표출순서',
  `WIDTH` varchar(3) NOT NULL DEFAULT '50' COMMENT 'Dashboard에 표시되는 화면 넓이 (50%, 100%)',
  `UDT_DT` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '수정일자',
  `UDT_ID` varchar(20) DEFAULT NULL COMMENT '변경자 ID',
  `CRT_ID` varchar(20) DEFAULT NULL COMMENT '생성자 ID',
  `CRT_DT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '변경일자',
  `AUTH_CODE` varchar(20) NOT NULL COMMENT '권한코드',
  PRIMARY KEY (`SEQ`)
) ENGINE=InnoDB AUTO_INCREMENT=21 DEFAULT CHARSET=utf8 COMMENT='게시판 목록 관리';

CREATE TABLE `CC_CATALOG` (
  `SCHEMA` varchar(128) NOT NULL COMMENT '스키마명',
  `TABLE` varchar(128) NOT NULL COMMENT '테이블명',
  `TABLE_COMMENT` varchar(512) DEFAULT NULL COMMENT '테이블설명',
  `COLUMN` varchar(128) NOT NULL COMMENT '컬럼명',
  `COLUMN_COMMENT` varchar(512) DEFAULT NULL COMMENT '컬럼설명',
  `DATA_TYPE` varchar(128) NOT NULL COMMENT '데이터 타입',
  `DATA_TYPE_LENGTH` int(10) unsigned DEFAULT NULL COMMENT '데이터 길이',
  `ORDER` int(10) unsigned DEFAULT NULL COMMENT '컬럼순서',
  `IS_PK` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'PK여부',
  `IS_NULLABLE` tinyint(1) NOT NULL DEFAULT '1' COMMENT 'NULL 여부',
  `COLUMN_DEFAULT` varchar(512) DEFAULT NULL COMMENT '컬럼 기본 값',
  `CRT_DT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '입력일자'
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='테이블관리(ETL에서 입력)';

CREATE TABLE `CC_COMMON_CODE` (
  `SEQ` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '공통코드 일련번호',
  `COMM_GRP_SEQ` int(10) unsigned NOT NULL COMMENT '공통코드그룹 일련번호',
