CREATE TABLE `CC_ITEM_MGMT` (
  `SEQ` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '연구항목 일련번호',
  `SCHEMA` varchar(128) NOT NULL COMMENT '스키마명',
  `TABLE` varchar(128) NOT NULL COMMENT '테이블명',
  `TABLE_COMMENT` varchar(512) DEFAULT NULL COMMENT '테이블설명',
  `COLUMN` varchar(128) NOT NULL COMMENT '컬럼명',
  `COLUMN_COMMENT` varchar(512) DEFAULT NULL COMMENT '컬럼설명',
  `DATA_TYPE` varchar(128) DEFAULT NULL COMMENT 'Data Type',
  `ITEM_NM` varchar(128) NOT NULL COMMENT '항목명',
  `ORDER` int(10) unsigned NOT NULL DEFAULT '1' COMMENT '출력순서',
  `SEARCH_YN` char(1) NOT NULL DEFAULT 'Y' COMMENT '검색여부 ==> ''Y'', ''N''',
  `ITEM_TYPE` char(3) NOT NULL DEFAULT 'TEX' COMMENT '항목종류 ==> DAT, COD, NUM, TEX',
  `CODE_TYPE` char(3) DEFAULT NULL COMMENT '항목이 코드일 경우 COD 종류. ''JSN'' : JSON, ''SQL'' : SQL',
  `CODE_SET` text COMMENT '"항목이 코드일 경우 코드 목록이 JSON값으로 정의됨. [{""M"":""남성"",""F"":""여성""}]\n항목을 선택하는 SQL문이 정의될수도 있음"',
  `POPUP_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '항목선택 팝업창 여부 (''Y'', ''N'')',
  `POPUP_PROGRAM_ID` varchar(128) DEFAULT NULL COMMENT 'POPUP=''Y''일 경우 팝업창 프로그램ID',
  `UPPER_TABLE` varchar(128) DEFAULT NULL COMMENT '상위테이블',
  `UPPER_COLUMN` varchar(128) DEFAULT NULL COMMENT '상위컬럼',
  `GMEC_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '중요여부',
  `HCC_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '다빈도 여부',
  `SYNONYM_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '동의어 적용 여부(TA에서 사용)',
  `ITEM_DESC` varchar(512) DEFAULT NULL COMMENT '항목설명',
  `CS_REPEAT_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '단면연구 반복데이터 조건 여부',
  `CH_DEFAULT_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '코호트연구 기본 연구항목 여부',
  `CH_CENSORING_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '코호트연구 중도절단 연구항목 여부',
  `UDT_DT` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '수정일자',
  `ITEM_CATE_SEQ` int(10) unsigned DEFAULT NULL,
  `ITEM_CATE_DETL_SEQ` int(10) unsigned DEFAULT NULL,
  `UDT_ID` varchar(20) DEFAULT NULL COMMENT '변경자 ID',
  `CRT_ID` varchar(20) DEFAULT NULL COMMENT '생성자 ID',
  `CRT_DT` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '변경일자',
  `ITEM_CATE` varchar(100) DEFAULT NULL,
  `ITEM_CATE_DETL` varchar(100) DEFAULT NULL,
  `BASE_DT_COLUMN` varchar(128) DEFAULT NULL COMMENT '기준일 컬럼명',
  `POPUP_COLUMN` varchar(128) DEFAULT NULL COMMENT '팝업호출시 WHERE조건으로 사용하기 위한 컬럼',
  `ITEM_FIRST_YN` char(1) NOT NULL DEFAULT 'N',
  `INSTCD_YN` char(1) DEFAULT 'N' COMMENT '사업장코드여부',
  `SEARCH_CNT` bigint(20) unsigned DEFAULT '0' COMMENT '조회수카운트',
  PRIMARY KEY (`SEQ`)
) ENGINE=InnoDB AUTO_INCREMENT=3963 DEFAULT CHARSET=utf8 COMMENT='연구항목 관리';

