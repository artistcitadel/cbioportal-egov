CREATE TABLE `CC_ITEM_CONT_DETL` (
  `SEQ` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '일련번호',
  `ITEM_SEQ` int(10) unsigned NOT NULL COMMENT '연구항목 일련번호',
  `CONT_SEQ` int(10) unsigned NOT NULL COMMENT '연구조건 일련번호',
  `PER_CODE` varchar(20) NOT NULL COMMENT '사용자ID',
  `ORDER` int(10) unsigned NOT NULL COMMENT '정렬순서',
  `INC_EXC` char(3) NOT NULL DEFAULT 'INC' COMMENT '조건의 포함/제외 여부. ''INC'': 포함, ''EXC'': 제외',
  `TAB_CD` varchar(2) NOT NULL COMMENT '"화면테이블구분\n공통 ==> C:환자선택, [RC,R]:연구항목, I:개인데이터\n단면 == > P : 반복데이터관리\n코호트 ==> RE : 사건정의, RQ : 중도절단\n사례대조 ==> CP: 모집단, CA : 사례군, CO : 대조군"',
  `GR_LV` int(10) unsigned NOT NULL DEFAULT '0' COMMENT '그룹레벨. 0: 그룹없음. 1~N : 그룹번호',
  `INPUT_VAL0` varchar(4000) DEFAULT NULL COMMENT '입력값 종류 : year, like, <, >, day, between, NULL',
  `INPUT_VAL1` varchar(1024) DEFAULT NULL COMMENT '입력값, 시작값',
  `INPUT_VAL2` varchar(1024) DEFAULT NULL COMMENT '입력 종료값',
  `CNT` int(10) unsigned DEFAULT NULL COMMENT '조회된 환자수',
  `AGG` varchar(5) DEFAULT NULL COMMENT '코호트 연구항목 기준 : first, last, 주기, 전체, 이전',
  `AND_OR` varchar(10) DEFAULT NULL COMMENT '조회 조건 : AND, OR, 주기번호(1주기, 2주기…)',
  `FIRST_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '기준일 최초 여부',
  `BASE_DT_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '기준일 여부',
  `RANGE_CD` char(1) DEFAULT NULL COMMENT '코호트 주기, 단면/사례대조 연구항목기준 범위',
  `RANGE_DN` int(10) unsigned DEFAULT NULL COMMENT '코호트 편차, 단면/사례대조 연구항목기준 편차',
  `DELETE_YN` char(1) NOT NULL DEFAULT 'N' COMMENT '삭제여부',
  `UDT_DT` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '수정일자',
