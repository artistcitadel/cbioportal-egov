	<!-- 트리 만들기 -->
	<select id="selectDiseaseCodeTreeW" parameterType="map" resultType="hashmap">
		SELECT
		    DISTINCT '전체공유조건' AS text,
		    #{code3} AS id,
		    '-1' AS parentid,
		    - 1 AS value,
		    '../../images/folder.png' AS icon
		FROM
		    DUAL
		UNION SELECT
		    LOV_NM AS text,
		    CONCAT(PER_CODE,'_',DEPT_CODE,'_',SEQ,'_',LOV_CD) AS id,
		    #{code3} AS parentid,
		    seq AS value,
		    '../../images/contactsIcon.png' AS icon
		FROM
		   ${tableNm}
		WHERE
		   LOV_CD = #{code3}
		UNION SELECT
		    DISTINCT '과공유조건' AS text,
		    #{code2} AS id,
		    '-2' AS parentid,
		    - 1 AS value,
		    '../../images/folder.png' AS icon
		FROM
		    DUAL
		UNION SELECT
		    LOV_NM AS text,
		    CONCAT(PER_CODE,'_',DEPT_CODE,'_',SEQ,'_',LOV_CD) AS id,
		    #{code2} AS parentid,
		    seq AS value,
		    '../../images/contactsIcon.png' AS icon
		FROM
		${tableNm}
		WHERE
		    TRIM(DEPT_CODE) = TRIM(#{deptCode})
		    AND LOV_CD = #{code2}
		UNION SELECT
		    DISTINCT '개인조건' AS text,
		    #{code1} AS id,
		    '-3' AS parentid,
		    - 1 AS value,
		    '../../images/folder.png' AS icon
		FROM
		    DUAL
		UNION SELECT
		    LOV_NM AS text,
		    CONCAT(PER_CODE,'_',DEPT_CODE,'_',SEQ,'_',LOV_CD) AS id,
		    #{code1} AS parentid,
		    seq AS value,
		    '../../images/contactsIcon.png' AS icon
		FROM
		${tableNm}
		WHERE
		    PER_CODE = TRIM(#{perCode})
		    AND LOV_CD = #{code1}
		ORDER BY
		    value,
		    parentid
		
	</select>
	
