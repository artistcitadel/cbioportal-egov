 	<!-- 상병코드 가져오기 -->
	<select id="selectDiseaseCodeW" parameterType="map" resultType="hashmap">
		SELECT 
  	  		A.DIS_CODE AS CODE,
			A.DIS_KNAM AS KNAM,
			A.DIS_ENAM AS ENAM,
			A.DIS_USED,
			A.DIS_INDEX,
			A.DIS_STND,
			A.DIS_RSFL,
			A.DIS_STFL,
			A.DIS_ABBR,
			A.DIS_SEQ,
			A.DIS_REMARK,
			A.DIS_FRDT,
			A.DIS_TODT,
			A.DIS_MINAGE,
			A.DIS_MAXAGE,
			A.DIS_SEX
  	  FROM
  	  		${SCHEMA}.CC_DISINX A
  	  WHERE
  	       1=1 
 	    <if test="SEARCH_VAL != null and SEARCH_VAL != ''">
 	    	<if test="SEARCH_KEY == 0">
	   		 AND UPPER(A.DIS_CODE) LIKE  '%' ||  UPPER(#{SEARCH_VAL}) || '%'
	   		 </if>
	   		 <if test="SEARCH_KEY == 1">
	   		 AND (UPPER(A.DIS_KNAM) LIKE  '%' ||  UPPER(#{SEARCH_VAL}) || '%'
	   		 OR UPPER(A.DIS_ENAM) LIKE  '%' ||  UPPER(#{SEARCH_VAL}) || '%')
	   		 </if>
	    </if> 
		
	</select>
	
