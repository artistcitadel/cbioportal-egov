	<!-- 키워드 대표어 -->
	<select id="selectKeyWordCodeList" parameterType="map" resultType="hashmap">
		SELECT 
			A.SEQ, A.TERM 
		FROM 
			CC_SYNONYM A
		LEFT OUTER JOIN CC_SYNONYM_ITEM_MAP B ON (A.SEQ=B.TERM_REP_SEQ)
		WHERE 1=1
			AND B.ITEM_CATE_SEQ	= #{item_cate_seq}
			AND B.ITEM_CATE_DETL_SEQ = #{item_cate_detl_seq}
			AND A.DELETE_YN = 'N'
		<if test="SEARCH_VAL != ''">
			AND A.TERM LIKE CONCAT('%',LOWER(#{SEARCH_VAL}),'%')
		</if>		
	</select>
	
