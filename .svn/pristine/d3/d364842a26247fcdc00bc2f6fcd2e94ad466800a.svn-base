	<!-- 처방코드 selectbox 뿌려주기 -->
	<select id="selectSelectBoxP" parameterType="map" resultType="hashmap">
		SELECT DISTINCT
	  	    		${columnName} AS CODE, NVL(${columnKorName},${columnName}) AS KNAM, NVL(${columnName} || ':' || ${columnKorName},${columnName}) as SUMNAM
			 FROM 
			 		${SCHEMA}.${tableName}
			 WHERE
			 	 TRIM(${whereColumnName}) ${logic} ${whereColumnData}
			 AND 
			 	  TRIM(${columnName}) != ''
			 AND 
			 	  TRIM(${columnKorName}) != ''
			 ORDER BY 
			 		CODE
	</select>
	
	<select id="selectCodeListPA" parameterType="map" resultType="hashmap">
		SELECT 
  	  		A.ORD_CODE AS CODE,
			A.ORD_KNAM AS KNAM,
			A.ORD_ENAM AS ENAM,
			A.ORD_SNAM,
		    A.ORD_ADAT,
		    A.ORD_ASAT,
			A.ORD_KIND,
			A.ORD_SCLS,
			A.ORD_TRDP,
		    CASE A.ORD_PRFL WHEN '1' THEN '가능' WHEN '2' THEN '불가능' END ORD_PRFL,
   			CASE A.ORD_GRFL WHEN '1' THEN '그룹' WHEN '2' THEN '싱글' END ORD_PRFL,
			A.ORD_BQTY,
			A.ORD_UNIT,
			A.ORD_DCFL,
			A.ORD_SPCD,
			A.ORD_SUCD,
			A.ORD_ETDT,
			A.ORD_ETPR 
	  FROM
			${SCHEMA}.CC_ORDR_CD A
  	  WHERE
  	       1=1 
  	  AND
  	  		TRIM(A.ORD_KIND) != ''
	 	    <if test="ORDKIND != null and ORDKIND != ''">
	  	    	AND A.ORD_KIND = #{ORDKIND}
			</if>  	       
	 	    <if test="ORDSCLS != null and ORDSCLS != ''">
	  	    	AND A.ORD_SCLS = #{ORDSCLS}
			</if> 
 	    <if test="SEARCH_VAL != null and SEARCH_VAL != ''">
 	    		<if test="SEARCH_KEY == 0">
	   		 AND (UPPER(A.ORD_KNAM) LIKE '%' ||   UPPER(#{SEARCH_VAL}) || '%' OR UPPER(A.ORD_ENAM) LIKE '%' ||   UPPER(#{SEARCH_VAL}) || '%') 
	   		 </if>
 	    	<if test="SEARCH_KEY == 1">
	   		 AND UPPER(A.ORD_CODE) LIKE  '%' ||  UPPER(#{SEARCH_VAL}) || '%'
	   		 </if>
	    </if> 
	</select>
	
