			FROM
			    CC_ITEM_MGMT A
			WHERE
				A.TABLE = (
					SELECT 
						UPPER_TABLE 
					FROM 
						CC_ITEM_MGMT 
					WHERE 
						ITEM_CATE_DETL_SEQ=#{item_cateDetl_seq} 
						AND SEQ=#{item_seq}
					)
				AND A.COLUMN = (
					SELECT 
						UPPER_COLUMN 
					FROM 
						CC_ITEM_MGMT 
					WHERE 
						ITEM_CATE_DETL_SEQ=#{item_cateDetl_seq} 
						AND SEQ=#{item_seq}
					)
	</select>
	
