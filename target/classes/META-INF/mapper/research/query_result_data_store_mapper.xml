<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC  
 "-//mybatis.org//DTD Mapper 3.0//EN"  
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
 
 <!-- 쿼리결과 저장 -->
 <mapper namespace="query_result_data_store">
	<insert id="createTableForResult" parameterType="map">
		${CREATE_TABLE_QUERY}
	</insert>
	
	<insert id="insertDataForResult" parameterType="map">
		${INSERT_QUERY}
	</insert>
	
	<!-- ITEM_CONT_DATA -->
	<insert id="insertItemContData" parameterType="map" useGeneratedKeys="true" keyProperty="SEQ">
		INSERT INTO CC_ITEM_CONT_DATA
		(
			 PER_CODE
			,CONT_SEQ
			,DATA_NM
			,DATA_SEQ
			,TABLE_ID
			,TABLE_CNT
			,TABLE_SIZE
			,UDT_DT
			,CRT_DT
			,TAB_CD
		)
		VALUES
		(
			 #{PER_CODE}
			,#{CONT_SEQ}
			,#{DATA_NM}
			,#{DATA_SEQ}
			,#{TABLE_ID}
			,#{TABLE_CNT}
			,#{TABLE_SIZE}	
			,NOW()
			,NOW()
			,#{TAB_CD}
		
		)	
	</insert>
	
	
	<insert id="insertItemContDataDetl" parameterType="map">
		INSERT INTO CC_ITEM_CONT_DATA_DETL 
		(
			DATA_SEQ,COLUMN_ID,COLUMN_COMMENT,UDT_DT,CRT_DT,
			OLAP_YN,ITEM_SEQ,DATA_TYPE,ITEM_TYPE,HIDDEN_YN
		)
		VALUES
		(
			#{DATA_SEQ},#{COLUMN_ID},#{COLUMN_COMMENT},	NOW()	,NOW(),
			#{OLAP_YN},
			#{ITEM_SEQ},
			#{DATA_TYPE},
			#{ITEM_TYPE},
			#{HIDDEN_YN}
		)
	</insert>
	



</mapper>
 
 
 